{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET"]
    }
  ],
  "crons": [
    {
      "path": "/sections/update-database/${CRON_SECRET_KEY}",
      "schedule": "5 * * * *"
    }
  ]
}
